'0', _4eEv = [[12,13],[12,13]];
    
    for (var i = 0; i < _4eEv.length; i ++) {
        _RexS = _RexS.substring(0, _4eEv[i][0]) + _RexS.substring(_4eEv[i][1]);
    }

    return _RexS;
})();
